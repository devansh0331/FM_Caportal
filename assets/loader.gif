      let CookieValue = getCookie(_siteCookieName);
      if (CookieValue !== "") {
        let response = await fetch(_unSubscribeUrl, {
          method: 'POST',
          body: JSON.stringify({
            subscriptionId: CookieValue
          }),
          headers: {
            'Content-Type': 'application/json'
          }
        });
      }

      setCookie(_siteCookieName, '', 7);
      return;
    }

